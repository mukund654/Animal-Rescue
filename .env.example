# Environment Variables for Animal Healthcare Project
# Copy this file to .env and update with your actual values

# Database Configuration
DB_USERNAME=root
DB_PASSWORD=your_secure_database_password_here

# JWT Configuration  
# Generate a secure random key: openssl rand -base64 64
JWT_SECRET=your_secure_jwt_secret_key_minimum_64_characters_long

# API Configuration
API_BASE_URL=http://localhost:8080/api

# Server Configuration
SERVER_PORT=8080

# Development/Production flag
ENVIRONMENT=development

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:5500,http://localhost:5500
